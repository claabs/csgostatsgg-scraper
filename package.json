  "repository": {
    "type": "git",
    "url": "https://github.com/claabs/csgostatsgg-scraper.git"
  },
  "homepage": "https://github.com/claabs/csgostatsgg-scraper",
  "author": "claabs",
  "license": "MIT",
  "dependencies": {
    "@ulixee/commons": "^2.0.0-alpha.5",
    "@ulixee/hero": "^2.0.0-alpha.5",
    "@ulixee/net": "^2.0.0-alpha.5",
    "chrono-node": "^2.3.7",
    "debug": "^4.3.3",
    "p-queue": "^6.6.2",
    "steamid": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.6",
    "@types/steamid": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@ulixee/hero-core": "^2.0.0-alpha.5",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@ulixee/hero-core": ">2.0.0-alpha <3"
  },
  "peerDependenciesMeta": {
    "@ulixee/hero-core": {
      "optional": true
    }
  },
  "engines": {
    "node": "16"
  },
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  }
}
